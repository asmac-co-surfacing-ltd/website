{
    "version": 2,
    "name": "asmac-website",
    "public": false,
    "regions": [
      "lhr1"
    ],
    "alias": [
      "asmacsurfacing.co.uk"
    ],
    "builds": [
      {
        "src": "frontend/next.config.js",
        "use": "@now/next"
      }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend/$1",
            "headers": {
              "cache-control": "s-maxage=31536000, maxage=0, stale-while-revalidate"
            }
          }
    ]
}